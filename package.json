{
  "name": "lit-html-chrome-extension",
  "version": "0.0.1",
  "description": "lit-html based chrome extension demo and boilerplate",
  "scripts": {
    "build": "node utils/build.js",
    "start": "node utils/webserver.js",
    "test": "better-npm-run test",
    "lint": "npx eslint ./src/js --fix",
    "prettier": " npx prettier --write */*.*",
    "fix-lint": "npm run prettier && npm run lint",
    "coverage": "nyc npm run test"
  },
  "betterScripts": {
    "test": {
      "command": "mocha -r esm"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run fix-lint && npm test"
    }
  },
  "esm": {
    "cjs": true,
    "sourceMap": true
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-istanbul": "^6.0.0",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "esm": "^3.2.25",
    "file-loader": "6.2.0",
    "fs-extra": "8.1.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "^4.2.1",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "prettier": "1.19.1",
    "style-loader": "1.3.0",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "3.9.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "blueimp-md5": "^2.12.0",
    "haunted": "^4.6.3",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "lit-translate": "^1.1.20",
    "material-icons": "^0.3.1",
    "url-loader": "^4.1.1"
  }
}
